    "prebuild": "npm run-script clean && npm run-script lint",
    "build": "npx typescript --sourceMap",
    "build-dev": "npx typescript --sourceMap",
    "postbuild": "node ./minify.js",
    "prepublishOnly": "npm run-script build"
