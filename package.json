    "eslint-plugin-jsdoc": "^31.0.7",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "terser": "^5.5.1",
    "tslint": "^6.1.3",
    "typedoc": "^0.20.16",
    "typedoc-plugin-external-module-name": "^4.0.6",
    "typescript": "^4.1.3"
